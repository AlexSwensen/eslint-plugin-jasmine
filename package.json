{
  "name": "eslint-plugin-no-exclusive-tests",
  "version": "0.2.0",
  "description": "ESLint rule to disallow use of exclusive tests",
  "main": "lib/rules/no-exclusive-tests.js",
  "peerDependencies": {
    "eslint": ">=0.8.0"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "rule",
    "jasmine",
    "ddescribe",
    "iit"
  ],
  "author": "Tom Vincent <npm@tlvince.com> (http://tlvince.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tlvince/eslint-no-exclusive-tests.git"
  },
  "bugs": {
    "url": "https://github.com/tlvince/eslint-no-exclusive-tests/issues"
  },
  "homepage": "https://github.com/tlvince/eslint-no-exclusive-tests"
}
